{# Exponential G calculations #}
{% for sp_name, thermo in species_dict.items() %}
{% set low = thermo.data[0] %}
{% set high = thermo.data[1] %}
{% set temp_range = thermo["temperature-ranges"] %}
# {{ sp_name }}
for i in range(veclen):
    if T[i] < {{ temp_range[1] }}:
        smh = \
        {{ "%0.15e" % (low[6]-low[0]) }} \
        {{ '+' if low[0] >= 0 else '-' }} {{ "%0.15e" % abs(low[0]) }} * np.log(T[i]) \
        {{ '+' if low[1] >= 0 else '-' }} {{ "%0.15e" % abs(low[1]/2.0) }} * T[i] \
        {{ '+' if low[2] >= 0 else '-' }} {{ "%0.15e" % abs(low[2]/6.0) }} * T[i]**2 \
        {{ '+' if low[3] >= 0 else '-' }} {{ "%0.15e" % abs(low[3]/12.0) }} * T[i]**3 \
        {{ '+' if low[4] >= 0 else '-' }} {{ "%0.15e" % abs(low[4]/20.0) }} * T[i]**4 \
        {{ '-' if low[5] >= 0 else '+' }} {{ "%0.15e" % abs(low[5]) }} / T[i]
    else:
        smh = \
        {{ "%0.15e" % (high[6]-high[0]) }} \
        {{ '+' if high[0] >= 0 else '-' }} {{ "%0.15e" % abs(high[0]) }} * np.log(T[i]) \
        {{ '+' if high[1] >= 0 else '-' }} {{ "%0.15e" % abs(high[1]/2.0) }} * T[i] \
        {{ '+' if high[2] >= 0 else '-' }} {{ "%0.15e" % abs(high[2]/6.0) }} * T[i]**2 \
        {{ '+' if high[3] >= 0 else '-' }} {{ "%0.15e" % abs(high[3]/12.0) }} * T[i]**3 \
        {{ '+' if high[4] >= 0 else '-' }} {{ "%0.15e" % abs(high[4]/20.0) }} * T[i]**4 \
        {{ '-' if high[5] >= 0 else '+' }} {{ "%0.15e" % abs(high[5]) }} / T[i]
    EG[i,{{ loop.index0 }}] = np.exp(smh)
{% endfor %}

